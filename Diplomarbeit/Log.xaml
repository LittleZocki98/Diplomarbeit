<Window x:Class="Diplomarbeit.Log"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Diplomarbeit"
        mc:Ignorable="d"
        Title="Event Log" Height="350" Width="600" Loaded="Window_Loaded" Icon="Resources/Icon.png">
  <ScrollViewer Name="scrollViewer">
    <TextBlock x:Name="textBlock" HorizontalAlignment="Stretch" Margin="0" TextWrapping="Wrap" Text="Event Log" VerticalAlignment="Stretch"/>
  </ScrollViewer>
  
</Window>
